  "scripts": {
    "start": "cls && parcel serve src/pages/*.html",
    "build": "parcel build src/pages/*.html",
    "serve": "serve -s ./dist",
    "type-check": "tsc --noEmit --watch",
    "sass-check": "sass --watch ./dist",
    "clear": "rm -rf ./dist ./.cache ./.parcel-cache",
    "del:0": "echo \"deleting all dependencies\"",
    "del:1": "find . '(' '(' -name \"package-lock.json\" ')' -o '(' -name \"node_modules\" ')' ')'",
    "del:2": "find . '(' '(' -name \"package-lock.json\" ')' -o '(' -name \"node_modules\" ')' ')' -prune -print -exec rm -rf '{}' +",
    "delete": "npm run del:0 && npm run del:1 && npm run del:2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-typescript": "^7.18.4",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@parcel/babel-plugin-transform-runtime": "^2.6.2",
    "@parcel/babel-preset-env": "^2.6.2",
