    "@parcel/optimizer-cssnano": "^2.6.2",
    "@parcel/packager-raw-url": "^2.6.2",
    "@parcel/transformer-image": "^2.6.2",
    "@parcel/transformer-jsonld": "^2.6.2",
    "@parcel/transformer-postcss": "^2.6.2",
    "@parcel/transformer-posthtml": "2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@parcel/transformer-typescript-tsc": "2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
