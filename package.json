    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "parcel": "2.6.2",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "posthtml": "^0.16.6",
    "posthtml-expressions": "^1.9.0",
    "posthtml-include": "^1.7.4",
    "posthtml-inline-svg": "^0.2.0",
