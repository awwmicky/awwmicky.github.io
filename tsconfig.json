{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": ["ESNext","DOM"],
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*.ts"]
}